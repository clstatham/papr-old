graph sineosc_manual {
    @in {}
    @out { @out }
    #in {
        #freq = 440.0
    }
    #out {}
    ~ {
        create s: sin;
        create freq_c2a: c2a;
        freq_c2a.#e <- #freq;
        s.@input <- ((@TAU * @t) * freq_c2a.@a);
        @out <- s.@out;
    }
}

graph main {
    @in {}
    @out { @dac0 @dac1 }
    #in {
        #freq0(20.0 : 2000.0) = 440.0
    }
    #out {}
    ~ {
        create so0: sineosc_manual;
        so0.#freq <- #freq0;
        [@dac0 @dac1] <- so0.@out;
    }
}