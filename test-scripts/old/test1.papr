graph test {
    @in {}
    @out {}
    #in { 
        #in0 = 0.0
        #in1 = 0.5
        }
    #out { #out0 }
    ~ {
        #out0 <- (#in0 * #in1);
    }
}

graph main {
    @in {}
    @out { @dac0 @dac1 }
    #in {
        #ci0(0:100) = 50
        #ci1(100:200) = 150
        #ci2(200:300) = 250
        #ci3(300:400) = 350
    }
    #out { #co0 }
    ~ {
        create ts: test;
        ts.#in0 <- #ci0;
        #co0 <- (ts.#out0 + (#ci2 * #ci3));
    }
}
