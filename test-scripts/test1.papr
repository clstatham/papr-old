import "oscs.papr"
import "envs.papr"

graph Main {
    () -> (@dac0)
    ~ {
        let #fm_amt = #Slider<0 1, 1.0>();
        let #fm_mul = #Slider<0.25, 8, 1.0>();
        let #decay0 = #Slider<0.1, 2.0, 1.0>();
        let #decay1 = #Slider<0.1, 2.0, 1.0>();

        let #freq = 440.0;
        let @s1 = @SineFm(#fm_amt, #fm_mul * #freq, 0.0, 0.0);
        let @s0 = @SineFm(0.1, #freq, 1.0, @s1);
        let @res = @s0 ^ @s1;
        @dac0 = @res * @s0;
    }
}
